
page{
  background-color: #F6F6F6;
}
.wrap{
  width: 100%;
  height: 100%;
}
/* 商品信息 */
.index-message{
  width: 100%;
  height: 200rpx;
  position: relative;
  background-color: white;
}
.index-message-img{
  width: 140rpx;
  height: 140rpx;
  margin: 30rpx 29rpx 30rpx 30rpx;
}
.index-message-text{
  font-size:26rpx;
  color:#888888;
  position: absolute;
  top: 32rpx;
}
.index-message-text1{
  font-size:24rpx;
  color:#888888;
  position: absolute;
  right: 30rpx;
  top: 32rpx;
}
.index-message-text2{
  font-size:26rpx;
  color:#888888;
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
}
/* 货物状态  退款原因 */
.index-change{
  width: 100%;
  height: 160rpx;
  background-color: white;
  margin-top: 20rpx;
}
.index-change-status{
  width: 690rpx;
  height: 80rpx;
  border-bottom: 1rpx solid #F2F2F2;
  margin: auto;
  position: relative;
}
.change-status{
  font-size:26rpx;
  color:rgba(51,51,51,1);
  line-height: 80rpx;
}
.change-noget{
  font-size:28rpx;
  color:rgba(51,51,51,1);
  line-height: 80rpx;
  position: absolute;
  right: 17rpx;
}
.change-img{
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  right: 0;
  bottom: 28rpx;
}
.index-change-status1{
  width: 690rpx;
  height: 80rpx;
  margin: auto;
  position: relative;
}
.change-status1{
  font-size:26rpx;
  color:rgba(51,51,51,1);
  line-height: 80rpx;
}
.change-noget1{
  font-size:28rpx;
  color:#888888;
  line-height: 80rpx;
  position: absolute;
  right: 17rpx;
}
.change-img1{
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  right: 0;
  bottom: 28rpx;
}
/* 退款金额 退款说明 */
.index-dback{
  width: 100%;
  height: 200rpx;
  background-color: white;
  margin-top: 20rpx;
}
.index-dback-box{
  width: 690rpx;
  height: 120rpx;
  border-bottom: 1rpx solid #F2F2F2;
  margin: auto;
}
.dback-box-price{
  font-size:26rpx;
  color:rgba(51,51,51,1);
  display: inline-block;
  padding-top: 25rpx;
  box-sizing: border-box;
}
.dback-box-red{
  font-size:30rpx;
  color:rgba(254,69,69,1);
}
.dback-box-word{
  font-size:22rpx;
  color:rgba(205,205,205,1);
  padding-top: 14rpx;
}
.index-dback-boxone{
  width: 690rpx;
  height: 79rpx;
  margin: auto;
}
.dback-boxone-text{
  font-size:26rpx;
  color:rgba(51,51,51,1);
  line-height: 79rpx;
}
.dback-boxone-text1{
  font-size:24rpx;
  color:rgba(136,136,136,1);
  padding-left: 10rpx;
  box-sizing: border-box;
  display: inline-block;
}
/* 上传凭证 */
.index-download{
  width: 100%;
  height: 288rpx;
  margin-top: 13rpx;
  background-color: white;
}
.index-download-all{
  width: 690rpx;
  height: inherit;
  margin: auto;
}
.download-all-one{
  width: inherit;
  height: 75rpx;
}
.all-one-one{
  font-size:26rpx;
  color:rgba(51,51,51,1);
  display: inline-block;
  margin-top:15rpx;
}
.all-one-two{
  font-size:22rpx;
  color:rgba(205,205,205,1);
  display: inline-block;
  margin-left: 15rpx;
}
.download-all-two{
  width: 690rpx;
  height: 160rpx;
}
.next-box2-pic{
  display: inline-block;
  width: 160rpx;
  height: 160rpx;
  border: 2rpx dashed #CDCDCD;
  margin-right: 20rpx;
  text-align: center;
  position: relative;
}
.box2-pic-img{
  width: 57rpx;
  height: 47rpx;
  margin: 38rpx 51rpx 10rpx 52rpx;
}
.box2-pic-text{
  font-size:24rpx;
  color:rgba(191,191,191,1);
}
.delete{
  position: absolute;
  top: -5rpx;
  right: -5rpx;
  width: 30rpx;
  height: 30rpx;
  background-color:rgb(246,199,92);
  border-radius: 50%;
  color: #fff;
  font-size: 22rpx;
  line-height: 24rpx;
  text-align: center;
}
/* 提交 */
.index-submit{
  width:690rpx;
  height:80rpx;
  background:rgba(246,199,92,1);
  border-radius:40rpx;
  margin: 42rpx auto;
  text-align: center;
}
.index-submit>text{
  font-size:28rpx;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height: 80rpx;
}


