}

.bottom-word1 {
  font-size: 28rpx;
  color: rgba(136, 136, 136, 1);
  float: right;
}

.bottom-words {
  color: #888888;
  font-size: 28rpx;
}

.index-bottom-two {
  width: 690rpx;
  height: 78rpx;
  margin: auto;
  text-align: right;
}

.index-bottom-two>text {
  font-size: 22rpx;
  color: rgba(254, 69, 69, 1);
  line-height: 78rpx;
}

.con {
  color: #333333;
}

