  background-color: #f6f6f6;
}

/* 底部 */
.index-footer {
  width: 100%;
  height: 120rpx;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
  box-shadow: 0px 3px 13px 0px rgba(0, 0, 0, 0.1);

  display: flex;
  justify-content: flex-end;
  align-items: center;

}

.index-footer-button {
  width: 220rpx;
  /* padding: 0 60rpx; */
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 24rpx;
  text-align: center;
  margin-left: 20px;
}

.index-footer-button:last-child{
  margin-right: 29rpx;
}

.index-footer-quxiao {
  width: 220rpx;
  height: 60rpx;
  border: 1rpx solid rgba(205, 205, 205, 1);
  border-radius: 24rpx;
  text-align: center;
  line-height: 60rpx;
  /* margin: 30rpx 20rpx 30rpx 260rpx; */
  display: inline-block;
}

.index-footer-quxiao>text {
  font-size: 24rpx;
  color: rgba(136, 136, 136, 1);
}

.index-footer-gopay {
  width: 220rpx;
  height: 60rpx;
  border: 1rpx solid rgba(245, 174, 50, 1);
  border-radius: 24rpx;
  text-align: center;
  display: inline-block;
}

.index-footer-gopay>text {
  font-size: 24rpx;
  color: rgba(245, 174, 50, 1);
  line-height: 60rpx;
}

.yellow-operate-btn{
  border: #F5AE32;
  color: #F5AE32;
}

