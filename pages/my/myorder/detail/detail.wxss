  width: 100%;
  height: 244rpx;
}

.index-bottom-one {
  width: 690rpx;
  height: 165rpx;
  border-bottom: 1rpx solid #F2F2F2;
  margin: auto;
}

.bottom-one-box {
  padding-top: 20rpx;
