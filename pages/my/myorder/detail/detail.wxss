.index-bcon {
  width: 100%;
  /* height: 418rpx; */
  position: relative;
  margin-bottom: 120rpx;
}

.index-bcon-box {
  width: 690rpx;
  /* height: 369rpx; */
  margin: 34rpx auto;
}

.bcon-box {
  width: inherit;
  border-bottom: 1rpx solid #f2f2f2;
}

.bcon-box-word {
  line-height: 60rpx;
  font-size: 22rpx;
  color: rgba(136, 136, 136, 1);
  display: inline-block;
}

.bcon-box-con {
  display: inline-block;
  line-height: 60rpx;
  font-size: 22rpx;
  color: rgba(136, 136, 136, 1);
  margin-left: 25rpx;
}

.showmore {
  width: 27rpx;
  height: 23rpx;
  position: absolute;
  left: calc(50% - 13rpx);
  bottom: 10rpx;
}

/* 灰色的 */
.block-four {
  width: 100%;
  /* height: 200rpx; */
