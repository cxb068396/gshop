.container_box{
  display: flex;
  width: 100%;
}
.container_box image{
  width: 140rpx;
  height: 140rpx;
  margin-right: 30rpx;
}
.desc_info{
  display: flex;
  flex-direction: column;
}
.title{
  font-size: 26rpx;
  color: rgba(34, 34, 34, 1);
}
.des{
  width: 128rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: rgba(249, 249, 249, 1);
  border-radius: 5rpx;
  text-align: center;
  color: #999999;
  font-size: 22rpx;
  word-break:keep-all;          
  white-space:nowrap;    
  overflow:hidden;            
  text-overflow:ellipsis;      
  margin-top: 19rpx;  
}
.price{
  font-size: 24rpx;
  color: rgba(254, 69, 69, 1);
  display: inline-block;
  margin-top: 21rpx;
}
.num{
  font-size: 26rpx;
  color: rgba(136, 136, 136, 1);
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  margin-left: auto;

}
