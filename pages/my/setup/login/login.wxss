/* pages/my/setup/login/login.wxss */
.wrap{
  width: 100%;
  height: 100%;
}
.black{
  width: 100%;
  height: 20rpx;
  background-color: #f6f6f6;
}
/* 手机号 验证码 */
.index-container{
  width: 650rpx;
  height: 202rpx;
  margin: 99rpx auto;
  margin-bottom: unset;
}
.login-phone{
  width: inherit;
  height: 100rpx;
  border-bottom: 1rpx solid #eee;
  font-size: 28rpx;
  position: relative;
}
.login-phone>input{
  position: absolute;
  left: 25rpx;
  top: 37rpx;
}
.login-yzm{
  width: 151rpx;
  height: 40rpx;
  border-left: 1rpx solid #cdcdcd;
  display: inline-block;
  position: absolute;
  right: 80rpx;
  top: 32rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
}
/* 完成 */
.login-over{
  width:690rpx;
  height:80rpx;
  background:rgba(246,199,92,1);
  border-radius:40rpx;
  margin: 155rpx auto;
  font-size:28rpx;
  color:rgba(255,254,254,1);
  line-height: 80rpx;
  text-align: center;
}
/* 其他方式登录 */
.login-text{
  width:227rpx;
  height:57rpx;
  margin: 52rpx auto;
  font-size:28rpx;
  color:rgba(153,153,153,1);
  text-align: center;
  line-height: 57rpx;
}
/* 微信 */
.login-img{
  width: 128rpx;
  height: 128rpx;
  margin: 45rpx 311rpx 87rpx 311rpx;
}
/* 最底下 */
.login-box{
  width: 502rpx;
  height: 71rpx;
  margin: auto;
  padding-bottom: 50rpx;
}
.login-box-text1{
  font-size: 24rpx;
  color: #999;
  text-align: center;
}
.login-box-text2{
  color: #F6C75C;
  font-size: 24rpx;
  text-align: center;
  margin-top: 10rpx;
}